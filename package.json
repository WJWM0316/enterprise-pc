{
  "name": "enterprise-pc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "build:dev": "vue-cli-service build --mode dev",
    "build:pro": "vue-cli-service build --mode pro",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clipboard": "^2.0.1",
    "cropperjs": "^1.4.1",
    "echarts": "^4.1.0",
    "element-ui": "^2.4.5",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "qs": "^6.5.2",
    "swiper": "^4.3.5",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "wangeditor": "^2.1.23",
    "web-storage-cache": "^1.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.7",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.7",
    "@vue/cli-service": "^3.0.0-rc.7",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.3",
    "sass": "^1.13.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "arrow-body-style": [
        0,
        "as-needed"
      ],
      "no-console": "off",
      "quotes": [
        "error",
        "single"
      ],
      "generator-star-spacing": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-param-reassign": 0,
      "no-multi-str": "off",
      "no-multiple-empty-lines": [
        "off",
        {
          "max": 1,
          "maxEOF": 0
        }
      ],
      "no-new-func": "off",
      "no-new-object": "off",
      "no-new-require": "off",
      "no-tabs": "off",
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": true
        }
      ]
    },
    "globals": {
      "mapActions": false,
      "mapGetters": false,
      "mapMutations": false,
      "mapState": false
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "ahthor": "lphva"
}
